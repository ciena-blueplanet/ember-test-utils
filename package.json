{
  "name": "ember-test-utils",
  "version": "12.0.0",
  "description": "Utilities to assist with testing ember apps/addons",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- npm install && ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-test-utils.git"
  },
  "engines": {
    "node": ">= 6.9.1"
  },
  "author": "Sophy Pal (https://github.com/sophypal)",
  "contributors": [
    "Adam Meadows (https://github.com/job13er)",
    "Matt Dahl (https://github.com/sandersky)",
    "Michael Carroll (https://github.com/juwara0)",
    "Jeremy Brown (https://github.com/notmessenger)",
    "Phillip Plummer (https://github.com/theotherdude"
  ],
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.7.0",
    "chai-dom": "^1.8.1",
    "ember-cli": "~3.28.6",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-code-coverage": "0.3.12",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-data": "~3.3.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-mocha": "^0.16.0",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^4.0.0",
    "ember-source": "~3.3.0",
    "ember-try": "^1.1.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-macro-helpers": "2.0.1",
    "glob-all": "^3.1.0",
    "node-yaml": "^3.1.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "ember-test-utils": {
    "excludeFromEnvironments": [
      "production"
    ]
  }
}